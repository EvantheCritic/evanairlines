@model IEnumerable<EmployeeModel>

<h1>Choose employee or Add Logbook Entry</h1>

@foreach (var employee in Model)
{
    <a class="btn btn-secondary" asp-area="" asp-controller="Admin" asp-action="AdminProperties" asp-route-id="@employee.id">@employee.name</a>
}

<a class="btn btn-primary" asp-area="" asp-controller="Admin" asp-action="AddEntry">Add Logbook Entry</a>

